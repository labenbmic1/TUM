%% ================= Einfuegen von leeren Seiten (Twoside ?) ====================== %%
\makeatletter%                                                                      %%
\newcommand{\insertpage}{%                                                          %%
    \if@twoside%                                                                    %%
        {%                                                                          %%
            \cleardoubleemptypage%                                                  %%
            \thispagestyle{empty}%                                                  %%
            \quad%                                                                  %%
            \newpage%                                                               %%
            \cleardoubleemptypage%                                                  %%
        }%                                                                          %%
    \else%                                                                          %%
        \newpage%                                                                   %%
        \thispagestyle{empty}%                                                      %%
        \quad%                                                                      %%
        \newpage%                                                                   %%
    \fi%                                                                            %%
}%                                                                                  %%
\newcommand{\insertpageifodd}{%                                                     %%
    \if@twoside%                                                                    %%
        \ifthispageodd{\insertpage}{\newpage}%                                      %%
    \else%                                                                          %%
        \newpage%                                                                   %%
    \fi%                                                                            %%
}%                                                                                  %%
\newcommand{\insertpageifeven}{%                                                    %%
    \if@twoside%                                                                    %%
        \ifthispageodd{\newpage}{\insertpage}%                                      %%
    \else%                                                                          %%
        \newpage%                                                                   %%
    \fi%                                                                            %%
}%                                                                                  %%
\makeatother%                                                                       %%
