\ProvidesPackage{packages/tumcolors}[2018/02/26]%	  								%%
\ProcessOptions\relax%                                                              %%
%% xcolor [dvipsnames,rgb,table,svgnames] = 										%%
%% 		Driver-independent color extensions for (pdf)LATEX 							%%
%% and pdfLATEX 																	%%
\PassOptionsToPackage{%																%%
dvipsnames,rgb,table,svgnames%														%%
}{xcolor}%																			%%
\makeatletter%																		%%
\@ifpackageloaded{xcolor}{}{%														%%
	\RequirePackage{xcolor}%     													%%
}%																					%%
\makeatother%																		%%
%% colortbl = add colour to LATEX tables 											%%
\makeatletter%																		%%
\@ifpackageloaded{booktabs}{%                                                       %%
    \@ifpackageloaded{colortbl}{}{%													%%
	   \RequirePackage{colortbl}%     												%%
    }%																		    	%%
}{}%                                                                                %%
\makeatother%																		%%
\makeatletter%																  		%%
\let\@TUMcolorI\relax%                                                        		%%
\providecommand{\TUMcolorI}[1]{\gdef\@TUMcolorI{#1}}%                         		%%
\let\@TUMcolorII\relax%                                                        		%%
\providecommand{\TUMcolorII}[1]{\gdef\@TUMcolorII{#1}}%                        		%%
\let\@TUMcolorIII\relax%                                                       		%%
\providecommand{\TUMcolorIII}[1]{\gdef\@TUMcolorIII{#1}}%                         	%%
%% Define TUM corporate design colors, taken from:    								%%
%% http://portal.myTUM.de/corporatedesign/index_print/vorlagen/index_farben 		%%
%% Primary colors 																	%%
\definecolor{TUMBlack}{HTML}{000000}%												%%
\definecolor{TUMBlue}{HTML}{0065BD}%												%%
\definecolor{TUMWhite}{HTML}{FFFFFF}%												%%
%% Secondary colors 																%%
\definecolor{TUMSecondaryBlue}{HTML}{005293}%										%%
\definecolor{TUMSecondaryBlue2}{HTML}{003359}%										%%
\definecolor{TUMDarkGray}{HTML}{333333}%											%%
\definecolor{TUMGray}{HTML}{808080}%												%%
\definecolor{TUMLightGray}{HTML}{CCCCC6}%											%%
%% Accent colors 																	%%
\definecolor{TUMAccentGray}{HTML}{DAD7CB}%											%%
\definecolor{TUMAccentOrange}{HTML}{E37222}%										%%
\definecolor{TUMAccentGreen}{HTML}{A2AD00}%											%%
\definecolor{TUMAccentLightBlue}{HTML}{98C6EA}%										%%
\definecolor{TUMAccentBlue}{HTML}{64A0C8}%											%%
%% Further colors 																	%%
%% V-I	I-II	VI-III		II-IV 	III-V  	IV-VI
\definecolor{TUMcolorI}{RGB}{222, 80, 80}%  	Light Red                           %%
\definecolor{TUMcolorII}{RGB}{200,10,0}%		Dark Red  	                       	%%
\definecolor{TUMcolorIII}{RGB}{153, 0, 51}%  	Darkest Red - Violet                %%
\definecolor{TUMcolorIV}{RGB}{34,139,34}%  	    Forest Green                      	%%
\definecolor{TUMcolorV}{RGB}{101, 151, 1}%  	Lighter Forest Green                %%
\definecolor{TUMcolorVI}{RGB}{255, 204, 0}%  	Dark-Yellow             			%%
%% Storing the colors into commands 												%%
\TUMcolorI{TUMBlack}%                                                        		%%
\TUMcolorII{TUMBlue}%                                               	       		%%
\TUMcolorIII{TUMWhite}%                                         	           		%%
\ExplSyntaxOn%																		%%
\clist_new:N \l_TUM_color_clist%													%%
\clist_clear:N \l_TUM_color_clist%													%%
\clist_put_right:Nn \l_TUM_color_clist {TUMBlack,TUMBlue,TUMWhite,%					%%
TUMSecondaryBlue,TUMSecondaryBlue2,TUMDarkGray,TUMGray,TUMLightGray,%				%%
TUMAccentGray,TUMAccentOrange,TUMAccentGreen,TUMAccentLightBlue,TUMAccentBlue,		%%
TUMcolorI,TUMcolorII,TUMcolorIII,TUMcolorIV,TUMcolorV,TUMcolorVI}%					%%
\DeclareExpandableDocumentCommand{\TUMcolor}{m}%									%%
{%																					%%
	\clist_item:Nn {\l_TUM_color_clist}{#1}%										%%
}%																					%%
\ExplSyntaxOff%																		%%
\AtEndPreamble{%																	%%
	\@ifpackageloaded{pgf}{%
		\pgfplotscreateplotcyclelist{tumI}{%
  			{color = TUMBlue,        		mark = star,             	},%
  			{color = TUMSecondaryBlue,    	mark = o,                  	},%
  			{color = TUMSecondaryBlue2,   	mark = square,              },%
  			{color = TUMcolorIV,  			mark = triangle,           	},%
  			{color = TUMcolorV,  			mark = diamond,            	},%
  			{color = TUMAccentGreen, 		mark = |,                  	},%
  			{color = TUMcolorVI,       		mark = 10-pointed star,    	},%
  			{color = TUMAccentOrange,       mark = x,     			  	},%
  			{color = TUMcolorI,   			mark = asterisk,           	},%
  			{color = TUMcolorII,  			mark = pentagon,           	},%
  			{color = TUMcolorIII,  			mark = otimes,            	}%
		}%
	}{}%
}%																					%%
\makeatother%																  		%%
\endinput%																		  	%%